{
    "name": "musicspree",
    "version": "1.0.0",
    "description": "Automated music recommendation fetcher and playlist manager for Navidrome",
    "main": "dist/index.js",
    "scripts": {
        "build": "rolldown build",
        "start": "node dist/index.js",
        "dev": "rolldown build --watch & node --watch dist/index.js",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "navidrome",
        "lastfm",
        "music",
        "slskd",
        "beets"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "node-cron": "^3.0.3",
        "axios": "^1.6.0",
        "dotenv": "^16.3.1",
        "winston": "^3.11.0",
        "commander": "^11.1.0"
    },
    "devDependencies": {
        "@types/node": "^20.8.0",
        "@types/node-cron": "^3.0.8",
        "typescript": "^5.2.2",
        "rolldown": "^0.12.0"
    }
}